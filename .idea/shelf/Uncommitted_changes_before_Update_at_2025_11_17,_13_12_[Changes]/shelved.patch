-    // 菜单手风琴
-    uniqueOpened: createToggleHandler(() => settingStore.setUniqueOpened()),
-
-    // 显示菜单按钮
-    menuButton: createToggleHandler(() => settingStore.setButton()),
-
-    // 显示快速入口
-    fastEnter: createToggleHandler(() => settingStore.setFastEnter()),
