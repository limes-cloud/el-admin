-      // 如果有子菜单，先递归过滤子菜单
-      if (item.children && item.children.length > 0) {
-        const filteredChildren = filterEmptyMenus(item.children)
-        return {
-          ...item,
-          children: filteredChildren
-        }
-      }
-      return item
-    })
-    .filter((item) => {
-      // 如果定义了 children 属性（即使是空数组），说明这是一个目录菜单，应该保留
-      if ('children' in item) {
-        return true
-      }
