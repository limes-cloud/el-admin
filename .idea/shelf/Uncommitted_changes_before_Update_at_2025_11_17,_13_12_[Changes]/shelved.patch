-
-/**
- * 根据API响应更新分页信息
- */
-export const updatePaginationFromResponse = <T>(
-  pagination: Api.Common.PaginationParams,
-  response: ApiResponse<T>
-): void => {
-  pagination.total = response.total ?? pagination.total ?? 0
-
-  if (response.current !== undefined) {
