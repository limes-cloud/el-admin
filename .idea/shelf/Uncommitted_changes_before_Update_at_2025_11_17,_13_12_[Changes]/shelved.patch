-}
-
-// 处理单个代码块
-function processBlock(block: HTMLElement) {
-  if (isBlockProcessed(block)) {
-    return
-  }
-
-  try {
-    highlightCode(block)
-    insertLineNumbers(block)
-    addCopyButton(block)
-    markBlockAsProcessed(block)
