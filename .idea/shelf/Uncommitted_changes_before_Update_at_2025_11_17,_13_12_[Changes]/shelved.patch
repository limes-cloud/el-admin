-  if (unprocessedBlocks.length === 0) {
-    return
-  }
-
-  if (unprocessedBlocks.length <= 10) {
-    // 如果代码块数量少于等于10，直接处理所有代码块
-    unprocessedBlocks.forEach((block) => processBlock(block))
