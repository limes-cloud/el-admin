-
-  const props = withDefaults(defineProps<Props>(), {
-    speed: 70,
-    direction: 'left',
-    type: 'default',
-    showClose: false,
-    typewriter: false,
-    typewriterSpeed: 100
-  })
-
-  // 状态管理
-  const containerRef = ref<HTMLElement | null>(null)
