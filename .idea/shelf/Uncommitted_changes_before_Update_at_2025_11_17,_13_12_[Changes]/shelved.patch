-    // 关闭进度条
-    const settingStore = useSettingStore()
-    if (settingStore.showNprogress) {
-      NProgress.done()
-    }
-
-    // 关闭 loading 效果
-    if (pendingLoading.value) {
-      nextTick(() => {
-        loadingService.hideLoading()
-        pendingLoading.value = false
-      })
-    }
-  })
